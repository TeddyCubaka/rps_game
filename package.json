    "eslint-plugin-react": "^7.31.8",
    "prettier": "2.7.1"
  },
  "lint": "eslint .",
  "lint:fix": "eslint --fix",
  "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
  "description": "\"This is an group of games\"",
  "main": "index.js",
